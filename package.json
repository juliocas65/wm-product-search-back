{
  "name": "wm-product-search-back",
  "version": "0.0.1",
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "unit-test": "mocha --recursive ./app -t 10000",
    "functional-test": "npm-run-all -p cucumber-features -r",
    "coverage": "nyc --reporter=lcov --reporter=text mocha --recursive ./app/ -t 10000; cat ./coverage/lcov.info | sed s#$PWD#.# > ./coverage/lcov.info.tmp; mv ./coverage/lcov.info.tmp ./lcov.info; rm -R ./.nyc_output; rm -r ./coverage",
    "coverage-text": "nyc mocha --recursive ./app; rm -R ./.nyc_output",
    "eslint": "eslint app.js app/** --quiet",
    "check-push": "npm run eslint && npm run unit-test && npm run functional-test && git push",
    "cucumber-features": "cucumber.js features"
  },
  "dependencies": {
    "body-parser": "1.17.2",
    "cors": "2.8.3",
    "cucumber": "1.3.3",
    "cucumber-assert": "2.0.0",
    "cucumber-html-reporter": "^2.0.3",
    "dateformat": "3.0.2",
    "debug": "2.6.8",
    "express": "4.15.3",
    "express-routemap": "1.1.0",
    "helmet": "3.8.1",
    "moment": "2.19.1",
    "request": "2.83.0",
    "strong-soap": "1.2.6",
    "swagger-ui-express": "2.0.7",
    "mongodb": "2.2.33",
    "winston": "2.3.1",
    "winston-daily-rotate-file": "1.4.6",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "depcheck": "0.6.7",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "mocha": "3.5.0",
    "mockery": "2.1.0",
    "npm-check": "5.4.4",
    "npm-check-updates": "2.11.3",
    "npm-run-all": "4.0.2",
    "nyc": "11.2.1"
  }
}
